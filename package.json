{
  "name": "single-webpack-bundle-kit",
  "version": "0.0.1",
  "description": "A simplified kit for React applications.",
  "main": "./src/index.js",
  "repository": "https://github.com/mattcarlotta/single-webpack-bundle-kit",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "rimraf dist && NODE_ENV=production webpack -p",
    "dev": "NODE_ENV=development PORT=3000 webpack-dev-server",
    "format": "prettier --write 'src/**/*.js'",
    "lint:js": "eslint 'src/**/*.js' --fix",
    "lint:styles": "stylelint **/*.scss",
    "start": "HOST=http://localhost: NODE_ENV=production PORT=8080 node server.js"
  },
  "dependencies": {
    "alias-dirs": "^0.0.11",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.12.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-watch": "^6.0.1",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "react-dev-utils": "^10.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.17.2",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpackbar": "^4.0.0"
  }
}
