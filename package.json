{
  "name": "react-start-kit",
  "version": "2.0.0",
  "description": "A starter kit for React and Redux applications.",
  "main": "./src/index.js",
  "repository": "https://github.com/mattcarlotta/react-starter-kit",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "analyze": "better-npm-run analyze",
    "test": "better-npm-run test",
    "test:coverage": "better-npm-run test:coverage",
    "format": "prettier --write 'src/**/*.js'",
    "lint:js": "eslint 'src/**/*.js' --fix",
    "lint:styles": "stylelint **/*.scss"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start": {
      "command": "node server.js",
      "env": {
        "HOST": "http://localhost:",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "rimraf dist && webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "analyze": {
      "command": "rimraf dist && webpack",
      "env": {
        "NODE_ENV": "production",
        "NODE_ENV_DEV": "staging",
        "ANALYZE": true
      }
    },
    "test": {
      "command": "jest --config ./jest.json --watchAll",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "test:coverage": {
      "command": "jest --config ./jest.json --coverage",
      "env": {
        "NODE_ENV": "testing"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --single-quote",
      "eslint --fix",
      "jest --config ./jest.json /src",
      "git add"
    ],
    "*.scss": [
      "stylelint **/*.scss",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "better-npm-run": "^0.1.1",
    "connected-react-router": "^6.6.1",
    "express": "^4.16.4",
    "history": "^4.10.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.1",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-beta.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.8.2",
    "child_process": "^1.0.2",
    "copy-webpack-plugin": "^5.1.0",
    "css-loader": "^3.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
